<div id="content">
<h1>Edit Administrator: <%= @admin.title %> <%= @admin.full_name %></h1>
<%= error_messages_for("admin", :class => 'ie') %>
<%= form_for @admin, :as => :admin, :url => administrator_path(@admin), :html => {:method => :put} do |f| %>
<%= render(:partial => "form", :locals => {:f => f, :button => "Update Admin"}) %>
<% end %>
<%= link_to "Back", administrators_path %>
</div>
<div id="secondary">
<%= initial %>
</div>



